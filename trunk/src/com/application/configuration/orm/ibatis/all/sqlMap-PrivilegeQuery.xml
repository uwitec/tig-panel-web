	<select id="getUnitListCount"
		parameterClass="com.web.form.administration.Unit"
		resultClass="java.lang.Integer">
		SELECT COUNT(*) FROM MNGNODEINFOTB t
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="nodelevel">
				t.NODELEVEL = #nodelevel#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="parentnodeid">
				t.PARENTNODEID = #parentnodeid# 
			</isNotEmpty>
			<isNotEmpty prepend="and" property="nodename">
				t.NODENAME LIKE '%'||#nodename#||'%'
			</isNotEmpty>
		</dynamic>
	</select>

	<select id="getUnitList"
		parameterClass="com.web.form.administration.Unit"
		resultClass="com.web.form.base.PrivilegeQueryForm">
		<![CDATA[
			SELECT * FROM (SELECT a.*,ROWNUM AS rn FROM (
		]]>
		SELECT NODEID AS id, NODENAME AS name FROM MNGNODEINFOTB t
		<dynamic prepend="where">
			<isNotEmpty prepend="and" property="nodelevel">
				t.NODELEVEL = #nodelevel#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="parentnodeid">
				t.PARENTNODEID = #parentnodeid# 
			</isNotEmpty>
			<isNotEmpty prepend="and" property="nodename">
				t.NODENAME LIKE '%'||#nodename#||'%'
			</isNotEmpty>
		</dynamic>
		order by NODEID
		<![CDATA[
			) a WHERE ROWNUM <= #rowEnd#) b WHERE b.rn >= #rowStart#
		]]>
	</select>
	
